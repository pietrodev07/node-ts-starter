{
  "name": "node-ts-starter",
  "version": "1.0.0",
  "description": "Node Typescript Starter!",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "prepare": "husky",
    "prebuild": "rimraf dist",
    "build": "tsc && tsc-alias",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "biome:all": " biome check --write .",
    "biome:lint": "biome lint --write . ",
    "biome:format": "biome format --write .",
    "start:examples": "tsx examples/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/node": "^22.12.0",
    "husky": "^9.1.7",
    "pretty-quick": "^4.0.0",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
